	<queryString>
		<![CDATA[select concat_ws(' ',Nombre_1,Nombre_2,Apellido_1,Apellido_2) as Nombres, 
Clase_equipo as Equipo,
Descripcion_marca_equipo as Marca, 
Serial,
Fecha_hora as "Fecha de ingreso",
Descripcion_tipo_entrada_salida as "Entrada/Salida",
Num_ficha as Ficha
from persona_equipos PE
inner join persona P on (PE.Id_persona = P.Id_persona)
inner join equipos E on(PE.Id_equipos = E.Id_equipos)
inner join tipo_equipo TE on (E.Id_tipo_equipo = TE.Id_tipo_Equipo)
inner join marca_equipo ME on (E.Id_marca_equipo = ME.Id_marca_equipo)
inner join entrada_salida ES on (ES.Id_persona = P.Id_persona)
inner join tipo_entrada_salida TES on (TES.Id_tipo_entrada_salida = ES.Id_tipo_entrada_salida)
inner join ficha FI on (FI.Id_ficha = P.Id_ficha)
where FI.Num_ficha = $P{ficha} and Fecha_hora>=$P{fechad} and Fecha_hora<=$P{fechah} and (ES.Id_tipo_entrada_salida =$P{tipoentrada} or ES.Id_tipo_entrada_salida =$P{tiposalida}) ;


]]>
	</queryString>
